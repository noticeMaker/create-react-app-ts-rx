{
  "extends": [
    "tslint:recommended", 
    "tslint-react", 
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/configureStore.ts"

    ]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": [ true, "warning" ]
  }
}
